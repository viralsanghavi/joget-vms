                        "autoPopulate": "",
                        "id": "field20",
                        "label": "",
                        "value": "<head>\r\n<meta charset=\"UTF-8\">\r\n<title> Date Picker<\/title>\r\n<\/head>\r\n<body>\r\n\r\n<form id=\"my-form\">\r\n <input type=\"text\" id=\"date_picker1\" size=5>From Date:\r\n <br>\r\n <input type=\"text\" id=\"date_picker2\" size=5>To Date:size=5><br>\r\n <button id=\"getBetween\" onsubmit=\"return false;\" >Get Between Dates<\/button>\r\n\r\n<div id=\"results\"><\/div>\r\n\r\n<\/form>\r\n\r\n\r\n\r\n\r\n\r\n<script type=\"text/javascript\">\r\n\r\n$(document).ready(function () {\r\n\r\n$(\"#date_picker1\").datepicker({\r\n\r\ndateFormat: 'dd-mm-yy'\r\n\r\n})\r\n\r\n$(\"#date_picker2\").datepicker({\r\n\r\ndateFormat: 'dd-mm-yy'\r\n\r\n})\r\n\r\n$('#getBetween').click(function (e) {\r\n\r\nvar start = $(\"#date_picker1\").datepicker(\"getDate\"),\r\n\r\nend = $(\"#date_picker2\").datepicker(\"getDate\"),\r\n\r\ncurrentDate = new Date(start),\r\n\r\nbetween = [];\r\n\r\nwhile (currentDate <= end) {\r\n\r\nbetween.push(new Date(currentDate));\r\n\r\ncurrentDate.setDate(currentDate.getDate() + 1);\r\n\r\n}\r\n\r\n$(document).ready(function (e) {\r\n\r\n$(\"#results\").html(between.map(function (value) {\r\n\r\nreturn $('#results:last').after('<div class=\"block\">' + value + '<button class=\"remove\">x<\/button><\/div>')\r\n\r\n}));\r\n\r\n$('.remove').click(function () {\r\n\r\n$(this).parent().remove();\r\n\r\nconsole.log(between)\r\n\r\n})\r\n\r\n// e.preventDefault()\r\n\r\n});\r\n\r\ne.preventDefault()\r\n\r\n});\r\n\r\n\r\n\r\n})\r\n\r\n\r\n\r\n<\/script>\r\n\r\n<\/body>"
