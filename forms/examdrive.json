                    "properties": {
                        "autoPopulate": "",
                        "id": "field20",
                        "label": "",
                        "value": "<head>\r\n\r\n<meta charset=\"UTF-8\">\r\n\r\n<title> Date Picker<\/title>\r\n\r\n<link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css'>\r\n\r\n<\/head>\r\n\r\n<body>\r\n\r\n<div class=\"container\">\r\n\r\n<form id=\"my-form\">\r\n\r\n<h3> Date Picker<\/h3>\r\n\r\nFrom Date: <input type=\"text\" id=\"date_picker1\" size=9>\r\n\r\nTo Date: <input type=\"text\" id=\"date_picker2\" size=9>\r\n\r\n<button id=\"getBetween\" onsubmit=\"return false;\" >Get Between Dates<\/button>\r\n\r\n<div id=\"results\">\r\n\r\n<\/div>\r\n\r\n<\/form>\r\n\r\n<\/div>\r\n\r\n\r\n\r\n<script type=\"text/javascript\">\r\n\r\n$(document).ready(function () {\r\n\r\n$(\"#date_picker1\").datepicker({\r\n\r\ndateFormat: 'dd-mm-yy'\r\n\r\n})\r\n\r\n$(\"#date_picker2\").datepicker({\r\n\r\ndateFormat: 'dd-mm-yy'\r\n\r\n})\r\n\r\n$('#getBetween').click(function (e) {\r\n\r\nvar start = $(\"#date_picker1\").datepicker(\"getDate\"),\r\n\r\nend = $(\"#date_picker2\").datepicker(\"getDate\"),\r\n\r\ncurrentDate = new Date(start),\r\n\r\nbetween = [];\r\n\r\nwhile (currentDate <= end) {\r\n\r\nbetween.push(new Date(currentDate));\r\n\r\ncurrentDate.setDate(currentDate.getDate() + 1);\r\n\r\n}\r\n\r\n$(document).ready(function (e) {\r\n\r\n$(\"#results\").html(between.map(function (value) {\r\n\r\nreturn $('#results:last').after('<div class=\"block\">' + value + '<button class=\"remove\">x<\/button><\/div>')\r\n\r\n}));\r\n\r\n$('.remove').click(function () {\r\n\r\n$(this).parent().remove();\r\n\r\nconsole.log(between)\r\n\r\n})\r\n\r\n// e.preventDefault()\r\n\r\n});\r\n\r\ne.preventDefault()\r\n\r\n});\r\n\r\n\r\n\r\n})\r\n\r\n\r\n\r\n<\/script>\r\n\r\n<\/body>"
