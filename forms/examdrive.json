                        "autoPopulate": "",
                        "id": "field16",
                        "label": "",
                        "value": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    \r\n    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css'>\r\n<\/head>\r\n\r\n<body>\r\n    <div class=\"container\">\r\n        <form id=\"my-form\">\r\n            \r\n     <div>\r\n        From Date: <input type=\"text\" id=\"date_picker1\" size=9>\r\n        To Date: <input type=\"text\" id=\"date_picker2\" size=9>\r\n        <!-- <div id=date_selected><\/div> -->\r\n        <button id=\"getBetween\" onsubmit=\"return false;\" >Get Dates<\/button>\r\n<\/div>\r\n        <div id=\"results\">\r\n\r\n        <\/div>\r\n        <\/form>\r\n    <\/div>\r\n\r\n\r\n<script type=\"text/javascript\">\r\n        $(document).ready(function () {\r\n            $(\"#date_picker1\").datepicker({\r\n                dateFormat: 'dd-mm-yy'\r\n            })\r\n            $(\"#date_picker2\").datepicker({\r\n                dateFormat: 'dd-mm-yy'\r\n            })\r\n            $('#getBetween').click(function (e) {\r\n                var start = $(\"#date_picker1\").datepicker(\"getDate\"),\r\n                    end = $(\"#date_picker2\").datepicker(\"getDate\"),\r\n                    currentDate = new Date(start),\r\n                    between = [];\r\n                     \r\n\r\n                while (currentDate <= end) {\r\n                    between.push(new Date(currentDate));\r\n                    currentDate.setDate(currentDate.getDate() + 1);\r\n                }\r\n\r\n                $(document).ready(function (e) {\r\n                    $(\"#results\").html(between.map(function (value) {\r\n                        return $('#results:last').after('<div class=\"block\">' + value + '<button class=\"remove\">x<\/button><\/div>')\r\n                         \r\n                        \r\n                    }));\r\n                    $('.remove').click(function () {\r\n                        $(this).parent().remove();\r\n                        console.log(between)\r\n                    })\r\n                // e.preventDefault()\r\n                });\r\n            e.preventDefault()\r\n                \r\n\r\n            });\r\n\r\n\r\n        })\r\n// }) \r\n\r\n    <\/script>\r\n<\/body>\r\n\r\n<\/html>\r\n"
